<html>
	<head>
		<title>automatik</title>
		<link rel="stylesheet" type="text/css" href="/static/style.css" />
		<link rel="stylesheet" type="text/css" href="/static/font-awesome-4.4.0/css/font-awesome.css" />
	</head>

	<body>
		<div id="scroll-area">
			<div id="container">
				<% for (var roomID in data.rooms) { %>
					<div class="tile unselectable">
						<%= data.rooms[roomID].renderBox() %>
					</div>
				<% } %>

				<%? data.editable %>
					<div class="tile unselectable">
						<a class="add-tile" href="/rooms/add">
							<i class="fa fa-plus"></i>
						</a>
					</div>
				<%?%>
			</div>
		</div>
	</body>
</html>
